{
  "network": "livenet",
  "port": 3001,
  "services": [
    "bitcoind",
    "insight-api-dash",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "/bitcore/.bitcore/data",
        "exec": "/bitcore/.bitcore/data/dashd"
      }
    },
    "insight-api-dash": {
      "disableRateLimiter": true
    }
  }
}
